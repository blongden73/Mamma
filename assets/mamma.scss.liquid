    margin-top: 50px;

    li {
      border: 1px solid $m-blue;
      padding: 10px;
      margin-right: 10px;
    }

    .suprise-me {
      text-align: right;
      margin-right: 10px;
      margin-top: 6px;
    }
  }
}

.m-event-title {
  display: block;
  font-family:'Noe Display';
}

.m-calendar-wrapper {
  background-color: $m-cream;

  h1 {
    margin-left: 10px;
    margin-top: 10px;
  }

  ul {
    li {
      border-top: 1px solid $m-blue;
      padding: 10px;
      padding-bottom: 10px;
    }
  }
}

.m-discover-cta {
  position: relative;
  height: 80vh;

  img {
    width: 100%;
    height: 100%;
    object-fit: cover;
    object-position: center;
  }

  .page-width {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    width: 100%;

    @include media-query($medium-up){
      width: 980px;
    }

    @include media-query($widescreen){
      width: 1300px;
    }

    h1 {
      color: #fff;
      max-width: 188px;
    }

    p {
      color: #fff;
      max-width: 220px;
    }
  }
}

.m-practice-list {
  position: relative;

  &:before {
    position: absolute;
    right: 0;
    left: 25%;
    background-color: $m-cream;
    bottom: 0;
    top: 0;
    z-index: -1;
    content: '';
    display: block;
  }

  .flex {
    justify-content: space-between;
  }

  .m-practice-list-header {
    text-align: center;

    span.handwritten {
      font-family: 'Run Wild';
    }

    h1 {
      padding-top: 20px;
    }
  }

  .m-practice-box {
    width: 30%;
    margin-right: 1.5%;
    background-color: $m-blue;
    text-align: center;
    padding: 10px;

    .m-practice-box-inner {
      border: 1px solid #fff;
      padding: 10px;
      min-height: 210px;
    }

    svg {
      margin-left: auto;
      margin-right: auto;
      display: block;
    }

    h1 {
      color: #fff;
    }

    a {
      color: #fff;
    }
  }
}

.m-people-group {
  display: flex;

  li {
    width: 16vw;
    height: 16vw;
    background-color: $m-blue;
  }

  .m-person {
    position: relative;

    .m-people-details {
      position: absolute;
      bottom: 0;
    }
    .m-people-name, .m-people-practice {
      background-color: $m-blue;
      color: $m-cream;
      padding: 6px;
      z-index: 10;
      display: block;
      width: max-content;
    }
    img {
      width: 100%;
      height: 100%;
      object-fit: cover;
      object-position: center;
    }
  }
}
.m-practioner-list {
  .m-practice-list-header {
    text-align: center;

    span.handwritten {
      font-family: 'Run Wild';
    }
  }
}

//general spacing
.m-practice-list-header, .m-calendar-header {
  margin-top: 40px;
}

.m-practice-list-header {
  h1 {
    padding-bottom: 40px;
  }
}

.m-calendar {
  margin-bottom: 40px;
}


//footer styling

.site-footer {
  background-color: $m-cream !important;

  .site-footer__linklist-item {
    font-family:'Noe Display';
    font-size: 18px;
    font-weight: 900;
    padding-right: 5px;
    color: $m-blue;
  }
}

.site-footer__content {
  flex-direction: column-reverse;
  text-align: center;

  .site-footer__item {
    padding: 0;
  }
}

.site-footer__item-inner--link_list {
  margin-left: auto;
  margin-right: auto;
}

.footer-logo {
  width: 300px;
  margin-left: auto;
  margin-right: auto;
}

.newsletter__submit {
  border: 1px solid $m-blue;
  background-color: transparent;
  font-family:'Noe Text';
  text-transform: capitalize;
  letter-spacing: 0;
  color: $m-blue;
}

.site-footer__item-inner--newsletter {
  .newsletter__input {
    margin-right: 20px;
  }

  h4 {
    float: left;
    font-size: 30px;
    margin-top: 10px;
  }

  form {
    float: right;
    width: 80%;
  }
}

.m-page-header {
  height: 100vh;
  width: 100%;

  .header-left {
    width: 50%;
    position: relative;

    img {
      width: 100%;
      height: 100%;
      object-fit: cover;
      object-position: center;
    }

    .header-inner {
      h1 {
        position: absolute;
        top: 50%;
        left: 50%;
        transform: translate(-50%, -50%);
        color: $m-cream;
      }
    }
  }

  .header-right {
    width: 50%;
    position: relative;

    .header-inner-content {
      position: absolute;
      top: 50%;
      left: 50%;
      transform: translate(-50%, -50%);
      width: 100%;

      h1 {
        text-align: center;
      }

      form {
        width: 100%;

        label {
          font-weight: 900;
          font-family:'Noe Display';
          margin-top: 20px;
          margin-bottom: 10px;
        }

        input {
          width: 100%;
          border: none;
          border-bottom: 1px solid $m-blue;
          font-family:'Noe Text';
        }

        select {
          width: 100%;
          font-family:'Noe Text';
        }

        button {
          background-color: $m-blue;
          border: none;
          color: $m-cream;
          font-family:'Noe Text';
        }
      }
    }
  }

  .header-inner {
    position: absolute;
    width: 90%;
    top: 50%;
    left: 50%;
    height: 90%;
    transform: translate(-50%, -50%);
  }
}
