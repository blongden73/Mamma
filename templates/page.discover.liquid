<div class="m-calendar-header">
  <div class="page-width">
    <div class="flex">
      <h1>Our Calendar</h1>
      <div class="m-filters">
        <ul>
          <li>All</li>
          <li>Festivals</li>
          <li>Retreats</li>
          <li>Classes</li>
          <li>Courses</li>
        </ul>
        <div class="suprise-me">
          <p>Don’t know, suprise me</p>
        </div>
      </div>
    </div>
  </div>
</div>
<div class="m-calendar">
  <div class="page-width">
    <div class="m-calendar-wrapper">
      <div class="flex">
        <h1>All Practices</h1>
        <ul>

        </ul>
      </div>
      <ul>
        {% for events in collections.all.products %}
          {% assign practition = events.metafields.airf.practition %}
          {% assign level = events.metafields.airf.level %}
          {% assign pregnancy_ = events.metafields.airf.pregnancy_ %}
          {% assign speed = events.metafields.airf.speed %}
          {% assign space = events.metafields.airf.space %}
          {% assign type = events.metafields.airf.type %}
          {% assign practice = events.metafields.airf.practice %}
          <li>
            <span class="m-event-title">{{events.title}}</span>
            <span class="m-event-meta">{{practition}} • {{speed}} • {{space}} • {{type}} • {{practice}} • {% if pregnancy_  == "true" %}Pregnancy{% endif %}</span>
            <div class="m-button">Book now</div>
          </li>
        {% endfor %}
      </ul>
    </div>
  </div>
</div>
<div class="m-discover-cta">
  <div class="page-width">
    <h1>
      Discover
      The Space by
      Mamma
    </h1>
    <p>Stay inspired and connect, watch online wellness and relax with healing sounds.</p>
  </div>
  <img src="https://cdn.shopify.com/s/files/1/0459/1697/9349/files/the_space_low.jpg?v=1600020411">
</div>
<div class="m-practice-list">
  <div class="flex">
    {% assign mind = linklists.mind %}
    {% assign body = linklists.body %}
    {% assign spirit = linklists.spirit %}
    <div class="m-practice-box">
      <h1>{{mind.title}}</h1>
      {{mind}}
    </div>
    <div class="m-practice-box">
      <h1>{{body.title}}</h1>
      {{body}}
    </div>
    <div class="m-practice-box">
      <h1>{{spirit.title}}</h1>
      {{spirit}}
    </div>
  </div>
</div>
