<div class="m-page-carousel">
  <div class="m-page-header flex">
    <div class="header-left">
      <div class="header-inner">
        <h1>Your one stop platform for <span class="underline">wellbeing</span></h1>
      </div>
      <div class="img-wrapper">
        {% assign page_image = page.metafields.airf.page_image %}
        {% if page_image and page_image != '&nbsp' %}
            <img class="car_img display" src="{{ page_image }}" />
        {% endif %}
        {% assign page_ima_2 = page.metafields.airf.page_ima_2 %}
        {% if page_ima_2 and page_ima_2 != '&nbsp' %}
            <img class="car_img" src="{{ page_ima_2 }}" />
        {% endif %}
        {% assign page_im_th = page.metafields.airf.page_im_th %}
        {% if page_im_th and page_im_th != '&nbsp' %}
            <img class="car_img" src="{{ page_im_th }}" />
        {% endif %}
      </div>
      <div class="carousel-footer">
        <p>Mind - Body - Spirit</p>
        <div class="m-blob">
          <div class="blob display"></div>
          <div class="blob"></div>
          <div class="blob"></div>
        </div>
      </div>
    </div>
    <div class="header-right">
      <div class="header-inner">
        <div class="header-inner-content">
          <h1>What are you looking for?</h1>
          <form class="search-form search-bar__form" action="{{ routes.search_url }}" method="get" role="search">
            <div class="search-form__input-wrapper">
              <input
                type="text"
                name="q"
                placeholder="Wellness, Yoga, Mindfullness, Breathwork"
                role="combobox"
                aria-autocomplete="list"
                aria-owns="predictive-search-results"
                aria-expanded="false"
                aria-label="{{ 'general.search.placeholder' | t }}"
                aria-haspopup="listbox"
                class="search-form__input search-bar__input"
                data-predictive-search-drawer-input
              />
              <input type="hidden" name="options[prefix]" value="last" aria-hidden="true" />
              <div class="predictive-search-wrapper predictive-search-wrapper--drawer" data-predictive-search-mount="drawer"></div>
            </div>

            <button class="search-bar__submit search-form__submit"
              type="submit"
              data-search-form-submit>
              {% include 'icon-search' %}
              <span class="icon__fallback-text">Search</span>
            </button>
          </form>
        </div>
      </div>
    </div>
  </div>
</div>
