<!-- Vue -->
{% assign slug = collection.handle %}
<div id="app" class="m-people-placeholder">
  <div class="m-page-header flex practitioner" v-for="practioner in practioners" v-if="practioner.slug === '{{slug}}'">
    <template>
      <div class="header-left">
        <div class="header-inner"></div>
        <img src="https://cdn.shopify.com/s/files/1/0459/1697/9349/files/madison-lavern-4gcqRf3-f2I-unsplash_1.jpg?v=1600357575">
      </div>
      <div class="header-right">
        <div class="header-inner">
          <div class="header-inner-content">
            <div class="practitioner-pager-header">
              <img src="https://cdn.shopify.com/s/files/1/0459/1697/9349/files/meet_practioners.png?v=1600117844">
            </div>
            <div class="m-practioner-info">
              <h1>${practioner.full_name}</h1>
              <h2>${practioner.practice}</h2>
              <p>${practioner.short_bio}</p>
              <div class="m-button">Book Now</div>
            </div>
          </div>
        </div>
      </div>
    </template>
  </div>
  <div class="m-people-info-gallery" v-for="practioner in practioners" v-if="practioner.slug === '{{slug}}'">
    <template>
      <div class="page-width">
        <div class="flex">
          <div class="m-people-info-left">
            <ul>
              <li>${practioner.bullet_1}</li>
              <li>${practioner.bullet_2}</li>
              <li>${practioner.bullet_3}</li>
            </ul>
            <p>${practioner.description}</p>
          </div>
          <div class="m-people-info-center">
            <p class="bold">${practioner.location}</p>
            <p class="bold">${practioner.how}</p>
            <p>${practioner.social_ig}</p>
            <p>${practioner.social_tw}</p>
            <p>${practioner.social_fb}</p>
          </div>
          <div class="m-people-info-right">
            <div class="m-people-info-gallery-inner">
              <div><img src="https://cdn.shopify.com/s/files/1/0459/1697/9349/files/madison-lavern-4gcqRf3-f2I-unsplash_1.jpg?v=1600357575"></div>
              <div><img src="https://cdn.shopify.com/s/files/1/0459/1697/9349/files/jade-stephens-N21356amsyw-unsplash.jpg?v=1600357576"></div>
              <div><img src="https://cdn.shopify.com/s/files/1/0459/1697/9349/files/dane-wetton-t1NEMSm1rgI-unsplash.jpg?v=1600357575"></div>
              <div><img src="https://cdn.shopify.com/s/files/1/0459/1697/9349/files/the_space_low.jpg?v=1600020411"></div>
            </div>
          </div>
        </div>
      </div>
    </template>
  </div>
  <div class="m-people-testimonial" v-for="practioner in practioners" v-if="practioner.slug === '{{slug}}'">
    <template>
    <div class="page-width">
      <div class="flex">
        <div class="testimonial-image">
          <div class="testimonial-image-one">
            <img src="https://cdn.shopify.com/s/files/1/0459/1697/9349/files/jade-stephens-N21356amsyw-unsplash.jpg?v=1600357576">
          </div>
          <div class="testimonial-image-two">
            <img src="https://cdn.shopify.com/s/files/1/0459/1697/9349/files/madison-lavern-4gcqRf3-f2I-unsplash_1.jpg?v=1600357575">
          </div>
        </div>
        <div class="testimonial-text">
          <blockquote>${practioner.testimonial}</blockquote>
          <cite>Testimonial Name</cite>
        </div>
      </div>
    </div>
    </template>
  </div>
</div>

<!-- Shopify -->
{% case section.settings.grid %}
  {% when 2 %}
    {%- assign max_height = 530 -%}
  {% when 3 %}
    {%- assign max_height = 345 -%}
  {% when 4 %}
    {%- assign max_height = 250 -%}
  {% when 5 %}
    {%- assign max_height = 195 -%}
{% endcase %}

{% if section.settings.layout == 'grid' %}
  {%- assign limit = section.settings.grid | times: section.settings.rows -%}
{% else %}
  {%- assign limit = 16 -%}
{% endif %}

{% paginate collection.products by limit %}

<div id="{{ section.id }}" data-section-id="{{ section.id }}" data-section-type="collection-template">
  <div class="m-page-header flex">
    <div class="header-left">
      <div class="header-inner">
      </div>
      <img src="https://cdn.shopify.com/s/files/1/0459/1697/9349/files/jade-stephens-N21356amsyw-unsplash.jpg?v=1600357576" />
    </div>
    <div class="header-right">
      <div class="header-inner">
        <div class="header-inner-content">
          <h1 class="section-title">Session <span class="handwritten">Details</span></h1>
          {% for product in collection.products %}
            <div class="collections-product">
              <h1>{{product.title}}</h1>
              <h2>{{product.price | money }}</h2>
              <div class="m-button-wrapper">
                <a href="{{ product.url }}"><div class="m-button">Book now</div></a>
                <a href=""><div class="m-button">Book a discovery call</div></a>
              </div>
            </div>
          {% endfor %}
          {{ collection.description }}
        </div>
      </div>
    </div>
  </div>
  </div>
</div>

{% endpaginate %}



